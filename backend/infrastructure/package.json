{
  "name": "location-detection-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Location Detection AI",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:yolo": "cdk deploy --app 'npx ts-node bin/deploy-yolo.ts'",
    "synth": "cdk synth",
    "synth:yolo": "cdk synth --app 'npx ts-node bin/deploy-yolo.ts'",
    "diff": "cdk diff",
    "diff:yolo": "cdk diff --app 'npx ts-node bin/deploy-yolo.ts'",
    "destroy": "cdk destroy",
    "destroy:yolo": "cdk destroy --app 'npx ts-node bin/deploy-yolo.ts'"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.110.0",
    "typescript": "~5.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.110.0",
    "constructs": "^10.3.0"
  }
}

